{
  "name": "@jontcrawford/snippets",
  "private": false,
  "version": "0.1.3",
  "description": "Made with create-react-library",
  "author": "jonathantcrawford",
  "license": "MIT",
  "repository": "",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npx tailwindcss -i ./src/main.css -o ./dist/main.css --minify && microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.40",
    "microbundle-crl": "*",
    "npm-run-all": "^4.1.5"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
